# conditional operations

assert((true&&true)==true)
assert((true&&false)==false)
assert((false&&true)==false)
assert((false&&false)==false)
assert((true||false)==true)
assert((true||true)==true)
assert((false||false)==false)
assert((false||true)==true)
assert(null==null)
assert(!(null!=null))
assert(!(!(false||(true||false))))
assert(5>3 && 6<10 && (14>=13||13<=15))
assert('c'=='c'&&'p'!='q')

for(i=0;i<10;i+=1){
    t = func(){return i}()
    assert(t==i)
}